{
  "name": "project-artha-financial-assistant",
  "version": "1.0.0",
  "description": "Project Artha - Your AI Financial Assistant built with React and TypeScript",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "clean": "rm -rf dist node_modules/.vite",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod --dir=dist"
  },
  "keywords": [
    "finance",
    "ai",
    "assistant",
    "react",
    "typescript",
    "vite",
    "financial-planning",
    "dashboard",
    "charts"
  ],
  "author": "Project Artha Team",
  "license": "MIT",
  "homepage": "https://project-artha.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/Rexosaury/Project_Artha.git"
  },
  "bugs": {
    "url": "https://github.com/Rexosaury/Project_Artha/issues"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "chart.js": "^4.5.0",
    "lucide-react": "^0.525.0",
    "postcss": "^8.5.6",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  }
}
